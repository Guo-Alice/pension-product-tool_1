{
  "version": 2,
  "builds": [
    {
      "src": "dify_app/app.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python-3.9",
        "pipInstall": true,
        "installCommand": "python -m pip install --upgrade pip && pip install --no-cache-dir -r dify_app/requirements.txt",
        "startCommand": "gunicorn --bind 0.0.0.0:$PORT dify_app.app:app"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dify_app/app.py"
    }
  ]
}
